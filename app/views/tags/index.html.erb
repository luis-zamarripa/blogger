<h1>All Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
        <%= link_to tag.name, article_path(tag) %>
        <% if logged_in? %>
          <%= link_to "<---Delete", tag_path(tag), method: :delete, data: {confirm: "Are you sure you want to delete the #{tag.name} tag?"} %>
        <% end %>
    </li>
  <% end %>
</ul>